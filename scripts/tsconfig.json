  "include": ["./*.ts", "output/shoulders.ts", "output/chest.ts", "output/biceps.ts", "output/abs.ts", "output/upper-back.ts", "output/abductors.ts", "output/adductors.ts", "output/calves.ts", "output/forearms.ts", "output/glutes.ts", "output/hamstrings.ts", "output/hip-flexors.ts", "output/triceps.ts", "output/quads.ts"],
  "exclude": ["node_modules"]
} 